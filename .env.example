# Each release ID will be echoed back in response headers
RELEASE_ID_BLUE=
RELEASE_ID_GREEN=

# Internal port app listens on (defined inside image)
PORT=

SLACK_WEBHOOK_URL=https://hooks.slack.com/services/XXX/YYY/ZZZ
ERROR_RATE_THRESHOLD=2
WINDOW_SIZE=200
ALERT_COOLDOWN_SEC=300
